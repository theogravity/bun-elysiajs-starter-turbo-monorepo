{
  "name": "@internal/backend-client",
  "description": "Type-safe client for the backend API via Eden Treaty",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsdown",
    "build:dev": "hash-runner",
    "clean": "rm -rf .turbo node_modules dist",
    "verify-types": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@elysiajs/eden": "1.4.8",
    "elysia": "1.4.25"
  },
  "devDependencies": {
    "@internal/backend": "workspace:*",
    "@internal/tsconfig": "workspace:*",
    "hash-runner": "4.0.0",
    "tsdown": "0.12.5"
  }
}
